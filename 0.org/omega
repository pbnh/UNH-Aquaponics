FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 0 -1 0 0 0 0];

//internalField   uniform $turbulentOmega;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $turbulentOmegaInitial;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $turbulentOmegaInitial;
        value           $turbulentOmegaInitial;
    }

    water_body
    {
      type            omegaWallFunction;
      value           $turbulentOmegaBoundary;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}

